# ===========================================
# MusicMaster DJ Pool - Environment Variables
# ===========================================
# Copy this file to .env and fill in your values
# NEVER commit .env to version control

# --- SUPABASE ---
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# --- HETZNER STORAGE BOX ---
HETZNER_HOST=your-storagebox.your-storagebox.de
HETZNER_USER=your-username
HETZNER_PASS=your-password
HETZNER_PORT=23

# --- VERCEL/PRODUCTION ---
STORAGE_USER=your-username
STORAGE_PASS=your-password
STORAGE_HOST=your-storagebox.your-storagebox.de

# --- OPTIONAL ---
FRONTEND_URL=https://your-domain.com
NODE_ENV=development
